{
    "manifest_version": 3,
    "name": "Bookmark-It",
    "version": "0.0.2",
    "description": "A Chrome and Brave extension that keeps bookmarks cross browser in sync",
    "action": {
        "default_title": "Click to open panel"
    },
    "permissions": [
        "sidePanel"
    ],
    "background": {
        "service_worker": "src/sw.ts"
    },
    "side_panel": {
        "default_path": "index.html"
    }
}